{
  "name": "express-api",
  "version": "1.0.0",
  "description": "API для учебного проекта",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "start": "node dist/main.js",
    "dev": "nodemon",
    "dev:inspect": "nodemon -e ts,json --exec node --inspect=localhost:9222 -r ts-node/register src/main.ts",
    "lint": "eslint ./src/**",
    "test": "jest",
    "test:e2e": "jest --config jest.e2e.config.ts --coverage",
    "generate": "prisma generate",
    "lint:fix": "eslint ./src/** --fix",
    "build": "tsc"
  },
  "author": "Marat Zhanpeisov",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/themjdex/express-api-study"
  },
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "inversify": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "tslog": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "prisma": "^3.14.0",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
}
